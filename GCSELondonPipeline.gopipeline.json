        {
          "name": "defaultJob",
          "environment_variables": [],
          "tabs": [],
          "resources": [],
          "artifacts": [
            {
              "source": "data/data.xlsx",
              "destination": "artifact",
              "type": "build"
            }
          ],
          "properties": [],
          "run_instance_count": "0",
          "timeout": 0.0,
          "tasks": [
            {
              "command": "cmd",
              "timeout": -1.0,
              "arguments": [
                "/c",
