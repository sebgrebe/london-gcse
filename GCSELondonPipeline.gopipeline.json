{
  "group": "defaultGroup",
  "name": "GCSELondonPipeline",
  "label_template": "${COUNT}",
  "lock_behavior": "none",
  "environment_variables": [
    {
      "name": "PORT",
      "value": "3000"
    }
  ],
  "parameters": [],
  "materials": [
    {
      "url": "https://github.com/sebgrebe/london-gcse",
      "branch": "master",
      "shallow_clone": false,
      "filter": {
        "ignore": [],
        "whitelist": []
